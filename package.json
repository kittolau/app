{
  "name": "kitto-app",
  "version": "1.0.0",
  "description": "Interactive educational app with AR, games, and video player",
  "scripts": {
    "start": "npx http-server -p 8000 -c-1 --cors",
    "dev": "npx http-server -p 8000 -c-1 --cors -o /app",
    "serve": "python3 -m http.server 8000",
    "app": "npx http-server -p 8000 -c-1 --cors -o /app",
    "preview": "npx http-server -p 8000 -c-1 --cors -o /preview.html",
    "prod": "node server.js",
    "prod:production": "NODE_ENV=production node server.js"
  },
  "keywords": [
    "ar",
    "education",
    "games",
    "video-player"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "http-server": "^14.1.1"
  },
  "dependencies": {
    "gltf-pipeline": "^4.3.0"
  }
}
